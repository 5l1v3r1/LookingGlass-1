<ul class="nav nav-pills" style="text-align:center;">
  <% first = 0 %>
  
  <% @dataspec.tab_order.each do |tab| %>
    <% if first == 0 %>
      <%= render partial: 'docs/tab'+tab, locals: {active: true} %>
      <% first = 1 %>
    <% else %>
      <%= render partial: 'docs/tab'+tab, locals: {active: false} %>
    <% end %>
  <% end %>
</ul>
