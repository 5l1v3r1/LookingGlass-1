<% file_type = File.extname(URI.parse(path).path) %>
<% doc_location = @dataspec.local_or_remote %>

<% if doc_location == "doc_cloud" %>
  <% file_path = path+".json" %>
  <%= render partial: 'docs/doc_cloud_doc', locals: {file_path: file_path} %>
<% elsif doc_location == "local" %>
  <% file_path = "/attachments"+path %>
  <%= render partial: 'docs/local_remote_doc', locals: { file_type: file_type,
    file_path: file_path} %>
<% elsif doc_location == "remote" %>
  <% file_path = path %>
  <%= render partial: 'docs/local_remote_doc', locals: { file_type: file_type,
  file_path: file_path} %>
<% end %>
