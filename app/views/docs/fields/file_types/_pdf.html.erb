<!-- Shows a PDF attachment -->
<object class="show-document" data="<%= file %>" height="100%" width="100%"
	style="width: 100%; height: 100%" type="application/pdf">
  <embed src="<%= asset_path(file) %>" width="100%" height="100%" type="application/pdf" />
</object>

<br />
<%= render partial: "docs/fields/file_types/download", locals: {file: file,
    download_name: download_name} %>


