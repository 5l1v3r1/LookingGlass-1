<!-- Shows an Office document (including PDF view) -->
<% if lg_pdf_view[file.split("/").last] %>
  <% file_view_path = lg_pdf_view[file.split("/").last] %>
  <object class="show-document" data="<%= file_view_path %>?#zoom=page-width" type="application/pdf">
    <embed src="<%= asset_path(file_view_path) %>" type="application/pdf" />
  </object>
<% end %>
  
<br />
<%= render partial: "docs/fields/file_types/download", locals: {file: file,
    download_name: download_name} %>


