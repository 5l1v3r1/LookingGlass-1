jQuery(document).ready(function(){
  $('#docs').DataTable({
    dom: 'C<"clear">Rlfrtip',
    bFilter: false,
    order: [[0, "desc"]],
    destroy: true,
    bProcessing: true,
    bDeferRender: true,
    columnDefs: <%= File.read(JSON.parse(File.read("app/dataspec/importer.json")).first["Hide Columns Template"]) %>
  });
});

var data = <%= File.read(JSON.parse(File.read("app/dataspec/importer.json")).first["Data Template"]) %>;

jQuery(function() {
  return $('#ftypes').change(function() {
    var chosen, f, _i, _len;
    chosen = $('#ftypes :selected').text();
    for (_i = 0, _len = data.length; _i < _len; _i++) {
      f = data[_i];
      if (chosen === f["Human Readable Name"]) {
        $('.searchin').hide();
        $('#' + f["Field Name"]).show();
      }
    }
    if (chosen === "All") {
      $('.searchin').hide();
      return $('#q').show();
    }
  });
});
